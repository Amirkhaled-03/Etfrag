@model CategoryVM
@{
    ViewData["Title"] = "Add Category";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}


<div id="modal-add" class="zoom-anim-dialog modal" style="background-color: #131720">
    <h6 class="modal__title">Add Category</h6>
    <div>
        <form asp-action="Create" asp-controller="Category" class="sign__form">
            <partial name="PartialViews\CreateUpdatePartialView.cshtml" model="@Model" />
            <partial name="~/Views/Shared/_AddDismissButtonsPartialView.cshtml" />
        </form>
    </div>

</div>